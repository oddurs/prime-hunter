{
  "annotations": {
    "list": []
  },
  "description": "Darkreach distributed prime hunting platform - fleet overview, search progress, volunteer stats",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Fleet Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "title": "Connected Workers",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "darkreach_workers_connected",
          "legendFormat": "workers"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "green", "value": 3 }
            ]
          }
        }
      }
    },
    {
      "title": "Available Blocks",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "targets": [
        {
          "expr": "darkreach_work_blocks_available",
          "legendFormat": "available"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 1000 }
            ]
          }
        }
      }
    },
    {
      "title": "Claimed Blocks",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "targets": [
        {
          "expr": "darkreach_work_blocks_claimed",
          "legendFormat": "claimed"
        }
      ]
    },
    {
      "title": "Active Search Jobs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "darkreach_search_jobs_active",
          "legendFormat": "active"
        }
      ]
    },
    {
      "title": "CPU Usage",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "darkreach_cpu_usage_percent",
          "legendFormat": "cpu"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent",
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        }
      }
    },
    {
      "title": "Memory Usage",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "targets": [
        {
          "expr": "darkreach_memory_usage_percent",
          "legendFormat": "memory"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent",
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        }
      }
    },
    {
      "title": "Search Progress",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false
    },
    {
      "title": "Primes Found by Form",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "rate(darkreach_primes_found_total[5m])",
          "legendFormat": "{{ form }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "title": "Candidates Tested by Form",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "rate(darkreach_candidates_tested_total[5m])",
          "legendFormat": "{{ form }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "title": "Total Primes Found (cumulative)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "darkreach_primes_found_total",
          "legendFormat": "{{ form }}"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "title": "Total Candidates Tested (cumulative)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "targets": [
        {
          "expr": "darkreach_candidates_tested_total",
          "legendFormat": "{{ form }}"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" }
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["darkreach"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Darkreach Fleet Overview",
  "uid": "darkreach-fleet"
}
